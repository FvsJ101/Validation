{
    "name": "michael/validator",
    "description": "Validation package",
    "require-dev": {
        "symfony/var-dumper": "^5.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Michael",
            "email": "michael@frostweb.co.za"
        }
    ],
    "autoload":{
        "psr-4": {
            "App\\":"app"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.0"
    },
    "scripts": {
        "start": "php -S localhost:9000",
        "clean": "composer dump-autoload -o",
        "post-install-cmd": [
            "composer dump-autoload -o"
        ],
        "post-update-cmd": [
            "composer dump-autoload -o"
        ]
    }
}
